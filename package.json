{
  "name": "un19n",
  "version": "0.1.1",
  "description": "Quick i18n translation generator",
  "main": "dist/index.mjs",
  "scripts": {
    "dev": "vitest dev",
    "build": "tsup",
    "play": "pnpm -C --filter @un19n/playground run dev",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "translation",
    "i18n",
    "automation"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "author": "zekun.jin",
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "c12": "^1.9.0",
    "citty": "^0.1.6",
    "consola": "^2.15.3",
    "defu": "^6.1.4",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^5.0.1",
    "lodash.flatten": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "magic-regexp": "^0.7.0",
    "magic-string": "^0.30.0",
    "md5": "^2.3.0",
    "minimatch": "^9.0.1",
    "nanoid": "^3.3.6",
    "ofetch": "^1.0.1",
    "pathe": "^1.1.0",
    "strip-literal": "^2.0.0",
    "unplugin": "^1.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.flatten": "^4.4.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/md5": "^2.3.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.35.0",
    "taze": "^0.13.3",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
